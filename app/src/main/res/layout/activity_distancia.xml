        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/guideline10"
        app:layout_constraintBottom_toTopOf="@+id/guideline13"
        app:layout_constraintTop_toBottomOf="@+id/guideline14"
        app:layout_constraintEnd_toStartOf="@+id/guideline12"
        tools:layout_editor_absoluteY="174dp" />

    <TextView
        android:id="@+id/txtLatitud"
        android:layout_width="160dp"
        android:layout_height="25dp"
        android:text="Latitud"
        android:textSize="16dp"
        android:textStyle="italic"
        android:textColor="#F00E00"
        app:layout_constraintEnd_toStartOf="@+id/guideline11"
        app:layout_constraintStart_toStartOf="@+id/guideline12"
        app:layout_constraintTop_toTopOf="@+id/guideline14"
        app:layout_constraintBottom_toTopOf="@+id/txtLongitud"/>

    <TextView
        android:id="@+id/txtLongitud"
        android:layout_width="160dp"
        android:layout_height="25dp"
        android:text="Longitud"
        android:textSize="16dp"
        android:textStyle="italic"
        android:textColor="#0112F0"
        app:layout_constraintBottom_toTopOf="@+id/guideline13"
        app:layout_constraintEnd_toStartOf="@+id/guideline11"
        app:layout_constraintStart_toStartOf="@+id/guideline12"
        app:layout_constraintTop_toBottomOf="@+id/txtLatitud"/>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.050615594" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
