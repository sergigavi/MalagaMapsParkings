    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#DCFFCC"
    tools:context=".DistanciaActivity">


    <TextView
        android:id="@+id/tituloParkings"
        android:layout_width="296dp"
        android:layout_height="84dp"
        android:text="Parkings"
        android:textAlignment="center"
        android:textColor="#EB54FF"
        android:textSize="68dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@+id/guideline11"
        app:layout_constraintStart_toEndOf="@+id/guideline10"
        app:layout_constraintTop_toTopOf="@+id/guideline8"
        app:layout_constraintBottom_toTopOf="@+id/guideline9"/>

    <TextView
        android:id="@+id/titleCoords"
        android:layout_width="136dp"
